venyx:SelectPage(venyx.pages[1], true)

-- Thay đổi màu sắc tên menu liên tục
spawn(function()
    while wait() do
        hue = hue + 1
        if hue > 360 then hue = 0 end
        local color = Color3.fromHSV(hue/360, 1, 1)
        themes.AccentColor = color
        venyx:setTheme(themes)
    end
end)


-- Hàm isBallNear() (ví dụ)
-- Hàm này cần được điều chỉnh dựa trên trò chơi cụ thể
local function isBallNear()
    local character = game.Players.LocalPlayer.Character
    if character then
        local humanoidRootPart = character:FindFirstChild(MJIiFfFTDvDIxcOMEmdHrSZeYfiXbUWGkEsYkrJKogVaRaItalQEcKqRsOnrRrnjCHIzDNrRAdywNxnFqObFIxmasbOA('TMTlPNYyiRyzRcmwQoQVwqFdozKDlUiONtuPqThlDtWBxodHOQKOeJGSHVtYW5vaWRSb290UGFydA=='))
        if humanoidRootPart then
            local ball = game.Workspace:FindFirstChild(MJIiFfFTDvDIxcOMEmdHrSZeYfiXbUWGkEsYkrJKogVaRaItalQEcKqRsOnrRrnjCHIzDNrRAdywNxnFqObFIxmasbOA('DvhIWbRtyzLJzisJiwlsGVbIsNCtSocLQgPuxlayNthjJqEGUnswcXhQmFsbA==')) -- Thay MJIiFfFTDvDIxcOMEmdHrSZeYfiXbUWGkEsYkrJKogVaRaItalQEcKqRsOnrRrnjCHIzDNrRAdywNxnFqObFIxmasbOA('hHEqKZGxPZlEaofNumqyrkbdVFDbytXAEQZbiHGFDeGrHeoHTPmWSVyQmFsbA==') bằng tên của đối tượng bóng trong trò chơi
            if ball then
                local distance = (humanoidRootPart.Position - ball.Position).Magnitude
                return distance < 5 -- Thay 5 bằng khoảng cách mong muốn
            end
        end
    end
    return false
end    
